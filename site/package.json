{
  "name": "proweb-studio-3d-website",
  "version": "0.2.0",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "src/app/globals.css",
    "src/reportWebVitals.ts",
    "src/middleware.ts"
  ],
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "BUILD_TIME=$(node -e \"process.stdout.write(new Date().toISOString())\") next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:fix": "next lint --fix",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "analyze": "ANALYZE=true next build",
    "build:analyze": "ANALYZE=true next build",
    "build:prod": "NODE_ENV=production node scripts/validate-env.js && NODE_ENV=production next build",
    "dev:perf": "NODE_ENV=development PERFORMANCE_MONITORING=true next dev",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./reports/lighthouse.html --chrome-flags='--headless'",
    "bundle-stats": "npx bundle-stats .next/static/chunks/",
    "perf:test": "npm run build:prod && npm run lighthouse",
    "gen:icons": "tsx scripts/gen-icons.ts",
    "optimize:images": "node scripts/optimize-images.js",
    "verify:images": "node scripts/verify-optimized-images.js",
    "validate-env": "node scripts/validate-env.js",
    "analyze:generate": "node scripts/analyze-bundle-data.js",
    "bundle:compare": "node scripts/bundle-compare.js",
    "seo:preflight": "tsx scripts/seo-preflight.ts",
    "seo:validate-sitemap": "node scripts/validate-sitemap.mjs",
    "lhci:collect": "lhci collect --config=lighthouserc.json",
    "lhci:assert": "lhci assert --config=lighthouserc.json",
    "lhci:collect:mobile": "lhci collect --config=lighthouserc.mobile.json",
    "lhci:assert:mobile": "lhci assert --config=lighthouserc.mobile.json",
    "ci:perf": "npm run lhci:collect && npm run lhci:assert",
    "ci:perf:mobile": "npm run lhci:collect:mobile && npm run lhci:assert:mobile",
    "ci:quality-check": "npm run typecheck && npm run lint && npm run format:check && npm run test",
    "ci:build": "npm run build:analyze && npm run analyze:generate && npm run seo:preflight",
    "validate:regions": "node scripts/validate-regions.js",
    "perf:validate": "./scripts/validate-performance.sh",
    "perf:test:dutch": "LIGHTHOUSE_CONFIG=dutch npm run lighthouse",
    "perf:monitor": "npm run dev:perf && npm run perf:validate"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "next lint --fix"
    ]
  },
  "dependencies": {
    "@gltf-transform/core": "^4.2.1",
    "@gltf-transform/extensions": "^4.2.1",
    "@gltf-transform/functions": "^4.2.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/dompurify": "^3.0.5",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/og": "^0.8.5",
    "@vercel/speed-insights": "^1.2.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.23.13",
    "glob": "^11.0.3",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.544.0",
    "next": "^14.2.5",
    "nodemailer": "^6.9.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.5",
    "three": "^0.169.0",
    "three-stdlib": "^2.36.0",
    "web-vitals": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@lhci/cli": "^0.13.0",
    "@next/bundle-analyzer": "^15.5.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/three": "^0.169.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "cheerio": "^1.1.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5",
    "undici": "^7.16.0",
    "vitest": "^3.2.4"
  }
}
