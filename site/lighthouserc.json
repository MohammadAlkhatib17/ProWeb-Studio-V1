{
  "ci": {
    "collect": {
      "startServerCommand": "next start -p 4020",
      "url": [
        "http://localhost:4020/",
        "http://localhost:4020/diensten",
        "http://localhost:4020/werkwijze",
        "http://localhost:4020/over-ons",
        "http://localhost:4020/contact",
        "http://localhost:4020/privacy",
        "http://localhost:4020/voorwaarden"
      ],
      "numberOfRuns": 3,
      "settings": {
        "budgetsPath": "budgets.json",
        "preset": "desktop",
        "throttlingMethod": "devtools"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.90,
            "aggregationMethod": "median-run"
          }
        ],
        "categories:seo": [
          "error",
          {
            "minScore": 1.00,
            "aggregationMethod": "median-run"
          }
        ],
        "categories:best-practices": [
          "error",
          {
            "minScore": 0.95,
            "aggregationMethod": "median-run"
          }
        ],
        "server-response-time": [
          "warn",
          {
            "maxNumericValue": 1200
          }
        ],
        "total-byte-weight": [
          "warn",
          {
            "maxNumericValue": 1500000
          }
        ],
        "legacy-javascript": "warn",
        "uses-optimized-images": "warn",
        "uses-responsive-images": "warn",
        "uses-text-compression": "warn"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}